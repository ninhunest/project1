<% provide :title, t(".posts") %>
<h1><%= t ".all_posts" %></h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% if @microposts.any? %>
      <ol class="microposts">
        <%= render @microposts %>
      </ol>
      <%= will_paginate @microposts %>
    <% end %>
  </div>
</div>
